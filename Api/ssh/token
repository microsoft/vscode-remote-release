paths:
  /entity-information/v1/download-entities:
    get:
      tags:
      - entity-management-extract-controller
      summary: getFileFromS3
      operationId: getFileFromS3UsingGET_1
      parameters:
      - name: token
        in: query
        description: token
        required: true
        schema:
          type: string
      - name: api_key
        in: query
        description: api_key
        schema:
          type: string
      responses:
        200:
          description: OK
          content:
            application/zip:
        401:
          description: Unauthorized
          content: {}
        403:
          description: Forbidden
          content: {}
        404:
          description: Not Found
          content: {}
    post:
      tags:
      - entity-management-extract-controller
      summary: getFileFromS3
